                                :text           "Page 3"}
                 :page-4-image {:type    "image"
                                :x       0
                                :y       0
                                :scale-x 0.5
                                :scale-y 0.5
                                :width   1000
                                :height  650
                                :src     "/raw/flip_page/spread-2-left.png"}
                 :page-4-text  {:type           "text"
                                :x              50
                                :y              50
                                :font-size      90
                                :scale-x        0.5
                                :scale-y        0.5
